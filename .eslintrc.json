{
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 2019,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"env": {
		"browser": true,
		"es6": true
	},
	"plugins": [
		"import",
		"react"
	],
	"extends": [
		"eslint:recommended",
		"plugin:import/errors",
		"plugin:react/recommended"
	],
	"rules": {
		"no-console": ["warn", {"allow": ["warn", "error"] }],
		"no-var": "warn",
		"eqeqeq": "warn",
		"semi": ["warn", "always"],
		"no-spaced-func": "warn",
		"comma-style": "warn",
		"comma-spacing": "warn",
		"key-spacing": "warn",
		"react/prop-types": [0],
		"no-unused-vars": ["warn", {
			"ignoreRestSiblings": true,
			"varsIgnorePattern": "^_",
			"argsIgnorePattern": "^_"
		}]
	}
}
